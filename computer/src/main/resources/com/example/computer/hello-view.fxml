<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.computer.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <HBox alignment="CENTER">
        <Label text=" So dau tien : "/>
        <TextField fx:id="number1"/>
    </HBox>
    <HBox alignment="CENTER" >
        <Label text="So thu hai : "/>
        <TextField fx:id="number2"/>
    </HBox>
    <HBox alignment="CENTER" >
        <Label text="Chon loai phep tinh : "/>
        <TextField fx:id="sattus"/>
    </HBox>
<HBox alignment="CENTER">
    <Button text="Input" onAction="#Check"/>
    <Button text="Delete" onAction="#Delete"/>
</HBox>

</VBox>
